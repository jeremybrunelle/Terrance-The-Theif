{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "*",        
        "destination": "/benchmark1/index.html",
        "type": 301
      },
      {
        "source": "/testing",
        "destination": "/full_game/dist/index.html",
        "type": 301
      },
      {
        "source": "/benchmark1",
        "destination": "/benchmark1/index.html",
        "type": 301
      },
      {
        "source": "/benchmark2",
        "destination": "/benchmark2/dist/index.html",
        "type": 301
      },
      {
        "source": "/benchmark3",
        "destination": "/benchmark1/index.html",
        "type": 301
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5033
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
